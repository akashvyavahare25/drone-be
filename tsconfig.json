{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
   "noImplicitAny": false,
   "noLib": false,
    "removeComments": true,    
    "strict": false,
   "skipLibCheck": true, 
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "incremental":true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./"
  },
  "exclude": ["node_modules", "dist"]
}
